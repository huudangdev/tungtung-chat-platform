{
    "env": {
        "browser": false,
        "node": true
    },
    "rules": {
        "no-constant-condition": 0,
        "no-underscore-dangle": 0,
        "camelcase": 0,
        "quotes": 0,
        "no-process-exit": 0,
        "no-empty": 0,
        "new-cap": [2, {
            "newIsCap": true,
            "capIsNew": true,
            "newIsCapExceptions": [],
            "capIsNewExceptions": []
        }]
    }
}
